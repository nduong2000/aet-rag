# AET-RAG System Configuration
# Copy this file to .env and customize the values for your environment

# ChromaDB settings
CHROMA_DB_DIR=./chroma_db_data
COLLECTION_NAME=aetna_docs

# Model settings
EMBEDDING_MODEL=text-embedding-005
CHAT_MODEL=gemini-2.5-pro-preview-05-06

# Retrieval settings
MAX_RETRIEVAL_DOCS=50
SIMILARITY_THRESHOLD=0.3
CHUNK_SIZE=1500
CHUNK_OVERLAP=200

# GCP settings (usually auto-detected from credentials)
GOOGLE_CLOUD_PROJECT=aethrag2
GCP_LOCATION=us-east1

# Field definitions file
FIELD_DEFINITIONS_FILE=field_definitions.json

# Flask settings (for local development)
FLASK_ENV=development
FLASK_DEBUG=0

# Logging level (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO 